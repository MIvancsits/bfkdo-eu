@page "/importtestpersons/{KnowledgeTestId:int}"
@inject ISnackbar MudSnackbar

@inherits AuthenticationRazorPage
<div class="container">
    <div class="import">
<MudFileUpload T="IBrowserFile" Accept=".csv" FilesChanged="UploadFile" MaximumFileCount="1">
        <ButtonTemplate>
            <MudButton HtmlTag="label"
                       Variant="Variant.Filled"
                       Color="Color.Primary"
                       StartIcon="@Icons.Material.Filled.CloudUpload"
                       Style="width:400px;height:50px;border-radius:50px; margin-top:30%"
                       for="@context">
                Teilnehmer importieren
            </MudButton>
        </ButtonTemplate>
    </MudFileUpload>
    <br /><br />
<MudButton Variant="Variant.Filled" Color="Color.Secondary" Href="/dashboard">Abbrechen und zurück</MudButton>
</div>
</div>


